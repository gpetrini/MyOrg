@techreport{brittoModelosKaldorianosCrescimento2011,
  title = {{Modelos kaldorianos de crescimento e suas extens\~oes contempor\^aneas}},
  author = {Britto, Gustavo and Romero, Jo{\~a}o Prates},
  year = {2011},
  pages = {35},
  address = {{UFMG/Cedeplar}},
  abstract = {This paper presents growth models based on Kaldor\textasciiacute s inaugural lectures, as well as their most recent extensions. Beyond the display of the evolution of such models, the paper intends to demonstrate that the original kaldorian models have served as a solid foundation over which new formulations have been built, continually demonstrating the relevance of the author's ideas. The model's extensions, other than allowing for the incorporation of an increasing number of economic characteristics originally proposed by Kaldor, also have considerably increased the analytical scope of the field by establishing theoretical links with other schools of thought. A neo-schumpeterian view is possible via the introduction of the impacts of technological gaps and innovation into the models. In its turn, a structuralist perspective is obtained by decomposing trade elasticities. Finally, from kaleckian growth models, the importance of a multi-sectoral analysis and of distinct distributive regimes over economic growth is brought to the models.},
  file = {Britto e Romero - 2011 - TEXTO PARA DISCUSSÃO N° 449.pdf:/home/gpetrini/Zotero/storage/WSE2UZ45/Britto e Romero - 2011 - TEXTO PARA DISCUSSÃO N° 449.pdf:application/pdf},
  timestamp = {2020-02-20T20:17:44Z}
}

