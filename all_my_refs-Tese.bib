  file = {Baptista et al. - 2016 - Macroprudential Policy in an Agent-Based Model of .pdf:/home/gpetrini/Zotero/storage/M2MFJKF5/Baptista et al. - 2016 - Macroprudential Policy in an Agent-Based Model of .pdf:application/pdf},
