@article{serrano_observacoes_2011,
  title = {Observa\c{c}\~oes {{Sobre}} as {{Teorias}} Do {{Crescimento}}},
  author = {Serrano, Franklin},
  year = {2011},
  pages = {1--23},
  url = {http://www.excedente.org/wp-content/uploads/2014/09/observac3a7c3b5es-sobre-as-teorias-do-crescimento-agosto-2011.pdf},
  urldate = {2018-07-10},
  abstract = {O objetivo destas notas \'e duplo. Em primeiro lugar, apresentar da forma mais simples e b\'asica poss\'ivel os aspectos principais das diversas teorias do crescimento, tanto neocl\'assicas quanto heterodoxas. Em segundo lugar, novamente considerando apenas seus aspectos mais essenciais, fazer uma avalia\c{c}\~ao cr\'itica preliminar das diversas teorias, no que diz respeito tanto a consist\^encia l\'ogico-te\'orica quanto \`a poss\'ivel relev\^ancia para o entendimento de alguns fatos estilizados b\'asicos sobre o processo de desenvolvimento. II. As teorias neocl\'assicas do crescimento II.1 O mecanismo de mercado na abordagem neocl\'assica As teorias do crescimento da abordagem neocl\'assica (ou marginalista) s\~ao baseadas na vis\~ao da opera\c{c}\~ao normal do mecanismo de mercado erigida a partir do chamado princ\'ipio da substitui\c{c}\~ao. De acordo com esta vis\~ao, os chamados fatores de produ\c{c}\~ao seriam basicamente substitutos, tanto atrav\'es da substitui\c{c}\~ao direta na produ\c{c}\~ao, em fun\c{c}\~ao inversa de seus pre\c{c}os relativos, quanto por meio da substitui\c{c}\~ao indireta de fatores (que ocorre quando os consumidores substituem os produtos relativamente mais caros, por serem mais intensivos em fatores de produ\c{c}\~ao de pre\c{c}o relativamente mais alto, pelos mais baratos). A suposta exist\^encia em desta geral rela\c{c}\~ao inversa entre a quantidade utilizada de cada " fator de produ\c{c}\~ao " e seu pre\c{c}o relativos permitiria, sob condi\c{c}\~oes competitivas, tanto uma tend\^encia de longo prazo \`a plena utiliza\c{c}\~ao das dota\c{c}\~oes de todos os fatores de produ\c{c}\~ao quanto simultaneamente a caracteriza\c{c}\~ao de seus pre\c{c}os relativos como um \'indice de sua escassez relativa. Nesta abordagem, em condi\c{c}\~oes competitivas (isto \'e, com flexibilidade nominal e real de seus pre\c{c}os) todos os fatores de produ\c{c}\~ao que tenham um pre\c{c}o positivo s\~ao necessariamente escassos e tendem a ser plenamente empregados a longo prazo.},
  file = {Serrano_Unknown_Observações Sobre as Teorias do Crescimento:/home/gpetrini/Zotero/storage/PW4BB6C2/Serrano_Unknown_Observações Sobre as Teorias do Crescimento.pdf:application/pdf},
  journal = {Mimeo},
  keywords = {FICHAR,MIMEO},
  timestamp = {2018-07-15T13:29:06Z}
}

