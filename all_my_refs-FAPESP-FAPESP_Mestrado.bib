@inproceedings{moreira_o_2017,
  title = {O Debate Envolvendo o Efeito Acelerados e a Controv\'ersia Sobre o Modelo Do Supermultiplicador Sraffiano},
  booktitle = {Anais Do {{X Encontro Internacional}} Da {{Associa\c{c}\~ao Keynesiana Brasileira}}},
  author = {Moreira, Vivian Garrido and Serrano, Franklin},
  year = {2017},
  pages = {1--20},
  publisher = {{Even3}},
  address = {{Bras\'ilia (DF) UnB}},
  abstract = {RESUMO O trabalho discute os requisitos te\'oricos para o ajustamento da capacidade produtiva \`a tend\^encia da demanda, no contexto do modelo do supermultiplicador sraffiano, atrav\'es de sua controv\'ersia na literatura, que girou basicamente em torno da hip\'otese de acelerador r\'igido. Para alguns isto implicaria em cont\'inua plena utiliza\c{c}\~ao da capacidade, requerendo que a demanda se ajustasse \`a capacidade. Para outros, implicaria em excessiva sensibilidade do investimento \`a demanda, gerando instabilidade. Desenvolvimentos recentes, no entanto, confirmam que n\~ao h\'a suposi\c{c}\~ao de acelerador r\'igido, sendo o modelo plenamente compat\'ivel com o acelerador flex\'ivel e atendendo \`as condi\c{c}\~oes de crescimento liderado pela demanda. Palavras-Chave: demanda efetiva, lei de Say, acelerador, supermultiplicador. C\'odigos JEL: B51, E11, O41 THE DEBATE CONCERNING THE ACCELERATOR EFFECT AND THE CONTROVERSY ABOUT THE SRAFFIAN SUPERMULTIPLIER MODEL ABSTRACT The paper discusses the requirements for capacity to adjust to the trend of demand, in the context of the controversy over the sraffian supermultiplier model that has revolved basically around the assumption of a rigid accelerator. To some this would imply continuous full capacity utilization, which would require that demand adjusted itself to capacity. To others it would imply that investment is too sensitive to demand, generating instability. Recent developments, however, confirm that the model is fully compatible with flexible accelerator hypotheses and meets the conditions for demand led growth.},
  file = {Moreira, Serrano_2017_O debate envolvendo o efeito acelerados e a controvérsia sobre o modelo do supermultiplicador sraffiano:/home/gpetrini/Zotero/storage/JF6EVZVP/Moreira, Serrano_2017_O debate envolvendo o efeito acelerados e a controvérsia sobre o modelo do supermultiplicador sraffiano.pdf:application/pdf},
  keywords = {FICHAR,Principal,Rever editora},
  timestamp = {2018-07-15T13:27:16Z}
}

@techreport{nikiforos_comments_2018,
  title = {Some Comments on the {{Sraffian Supermultiplier}} Approach to Growth and Distribution},
  author = {Nikiforos, Michalis},
  year = {2018},
  pages = {1--23},
  address = {{Annandale-On-Hudson}},
  institution = {{Levy Economics Institute of Bard College}},
  abstract = {The paper discusses the Sraffian Supermultiplier (SSM) approach to growth and distribution. It makes five points. First, in the short run the role of autonomous expenditure can be appreciated within a standard post-Keynesian framework (Kaleckian, Kaldorian, Robinsonian, etc.). Second, and related to the first, the SSM model is a model of the long run and has to be evaluated as such. Third, in the long run, one way that capacity adjusts to demand is through an endogenous adjustment of the rate of utilization. Fourth, the SSM model is a peculiar way to reach what Garegnani called the " Second Keynesian Position. " Although, it respects the letter of the " Keynesian hypothesis, " it makes investment quasi-endogenous and subjects it to the growth of autonomous expenditure. Fifth, in the long run it is unlikely that " autonomous expenditure " is really autonomous. From a stock-flow consistent point of view this implies unrealistic adjustments after periods of changes in stock-flow ratios. Moreover, if we were to take this kind of adjustment at face value, there would be no space for Minskyan financial cycles. This also creates serious problems for the empirical validation of the model.},
  file = {Nikiforos_2018_Some comments on the Sraffian Supermultiplier approach to growth and distribution:/home/gpetrini/Zotero/storage/PR7MAPQY/Nikiforos_2018_Some comments on the Sraffian Supermultiplier approach to growth and distribution.pdf:application/pdf},
  keywords = {Complementar,METADATA,Rever lugar},
  number = {907},
  timestamp = {2018-07-11T18:37:46Z}
}

