@article{perez-montiel_2021_Housing,
  title = {Housing Is {{NOT ONLY}} the {{Business Cycle}}: {{A Luxemburg}}-{{Kalecki External Market Empirical Investigation}} for the {{United States}}},
  shorttitle = {Housing Is {{NOT ONLY}} the {{Business Cycle}}},
  author = {{P{\'e}rez-Montiel}, Jos{\'e} A. and Pariboni, Riccardo},
  year = {2021},
