@techreport{pariboniAutonomousDemandMarglinBhaduri2015,
  title = {Autonomous Demand and the {{Marglin}}-{{Bhaduri}} Model: A Critical Note},
  shorttitle = {Autonomous Demand and the {{Marglin}}-{{Bhaduri}} Model},
  author = {Pariboni, Riccardo},
  year = {2015},
  month = aug,
  institution = {{Department of Economics, University of Siena}},
  url = {https://ideas.repec.org/p/usi/wpaper/715.html},
  urldate = {2018-12-20},
  abstract = {Within Post-Keynesian macroeconomic theory, the contribution by Marglin and Bhaduri (Marglin and Bhaduri, 1990; Bhaduri and Marglin, 1990) on the relationship between income distribution and growth has progressively asserted itself as a benchmark model, a reference point that has originated and still gives rise to plenty of theoretical and empirical works. Given this popularity, in the related literature it is often claimed that the only open question left is an empirical one - to assess econometrically whether a particular economy is wage or profit-led. In this essay, I will argue that some theoretical issues, related to this model and to the literature inspired by it, can nonetheless be raised. In particular, the treatment of investment appears to be the least convincing aspect of the approach a l\`a Marglin-Bhaduri. More specifically, it seems possible to raise some doubts about an independent long-run influence of the profit rate or of the profit share on investment, influence that is not in general justified or explained in detail by this literature and that to some extent is simply taken for granted. It will be shown that, if the Marglin-Bhaduri model is integrated with an explicit consideration of the autonomous components of demand, income distribution does not exert any permanent influence on the rate of growth of the economy and on the rate of accumulation. Matching this result with the usual assumption, made in Post-Keynesian models of growth and distribution, that capacity utilization is the adjusting variable in equilibrating investment and savings leads to paradoxical results that question the plausibility of an accumulation function like the one used in the Marglin-Bhaduri model.},
  file = {Pariboni - 2015 - Autonomous demand and the Marglin-Bhaduri model a.pdf:/home/gpetrini/Zotero/storage/KVZQ2HSV/Pariboni - 2015 - Autonomous demand and the Marglin-Bhaduri model a.pdf:application/pdf},
  keywords = {Growth,Income distribution,Investment function,Marglin-Bhaduri},
  language = {en},
  number = {715},
  timestamp = {2018-12-20T13:35:26Z}
}

