@article{pedrosaFirmsLeverageRatio,
  title = {Firms' Leverage Ratio and the {{Financial Instability Hypothesis}} ({{FIH}}): An Empirical Investigation for the {{United States}} Economy (1970-2014)},
  author = {Pedrosa, Italo},
  pages = {36},
  abstract = {Reflecting the unsolved tensions about the transition from micro to macro results in Minsky's Financial Instability Hypothesis (FIH), there are many interpretations of how financial fragility builds up in his framework. A positively-sloped curve associating nonfinancial firms' leverage and financial fragility is the cornerstone of the many FIH's versions. Using firm-level and macroeconomic data to comply with the variety of FIH's interpretations, we empirically assess the relations between leverage and financial fragility \textendash conceptualised according to Minsky's scale (from the financially sound to the more fragile firms: hedge, speculative and Ponzi) \textendash{} in the United States economy (1970-2014). The main findings are: 1. the evolution of the aggregate leverage ratio does not account for the systemic financial fragility, measured by the frequency of speculative and Ponzi firms; 2. within the biggest firms, the leverage has increased along with the incidence of hedge financing, and for the smallest firms group the opposite has happened. We conclude that the positive relation among leverage and financial fragility cannot be deemed to be a general outcome.},
  file = {Pedrosa - Firms’ leverage ratio and the Financial Instabilit.pdf:/home/gpetrini/Zotero/storage/9PRZTIV8/Pedrosa - Firms’ leverage ratio and the Financial Instabilit.pdf:application/pdf},
