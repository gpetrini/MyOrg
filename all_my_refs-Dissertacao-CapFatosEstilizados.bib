
@article{aalbers_2008_Financializationa,
  ids = {aalbersFinancializationHomeMortgage2008,aalbersFinancializationHomeMortgage2008a},
  title = {The {{Financialization}} of {{Home}} and the {{Mortgage Market Crisis}}},
  author = {Aalbers, Manuel B.},
  year = {2008},
  month = jun,
  volume = {12},
  pages = {148--166},
  issn = {1024-5294},
  doi = {10.1179/102452908X289802},
  url = {https://journals.sagepub.com/doi/abs/10.1179/102452908X289802},
  urldate = {2019-07-04},
  abstract = {Financialization can be characterized as capital switching from the primary, secondary or tertiary circuit to the quaternary circuit of capital. Housing is a central aspect of financialization. The financialization of mortgage markets demands that not just homes but also homeowners become viewed as financially exploitable. It is exemplified by the securitization of mortgage loans, but also by the use of credit scoring and risk-based pricing. In the past century, mortgage markets were transformed from being a `facilitating market' for homeowners in need of credit to one increasingly facilitating global investment. Since mortgage markets are both local consumer markets and global investment markets, the dynamics of financialization and globalization directly relate homeowners to global investors thereby increasing the volatility in mortgage markets, as the current crisis shows all too well.},
  file = {Aalbers - 2008 - The Financialization of Home and the Mortgage Mark.pdf:/home/gpetrini/Zotero/storage/VID8V6Q5/Aalbers - 2008 - The Financialization of Home and the Mortgage Mark.pdf:application/pdf;Snapshot:/home/gpetrini/Zotero/storage/MAVJXYEB/search.html:text/html},
  journal = {Competition \& Change},
  keywords = {Fichado},
  language = {en},
  number = {2},
  timestamp = {2020-12-21T14:38:49Z}
}

@article{agnelloECONOMICACTIVITYCREDIT2018,
  title = {{{ECONOMIC ACTIVITY}}, {{CREDIT MARKET CONDITIONS}}, {{AND THE HOUSING MARKET}}},
  author = {Agnello, Luca and Castro, Vitor and Sousa, Ricardo M.},
  year = {2018},
  month = oct,
  volume = {22},
  pages = {1769--1789},
  issn = {1365-1005, 1469-8056},
  doi = {10.1017/S1365100516000869},
  url = {https://www.cambridge.org/core/product/identifier/S1365100516000869/type/journal_article},
  urldate = {2019-03-04},
  abstract = {In this paper, we assess the characteristics of the housing market and its main determinants. Using data for 20 industrial countries over the period 1970Q1\textendash 2012Q2 and a discrete-time Weibull duration model, we find that the likelihood of the end of a housing boom or a housing bust increases over time. Additionally, we show that the different phases of the housing market cycle are strongly dependent on the economic activity, but credit market conditions are particularly important in the case of housing booms. The empirical findings also indicate that although housing booms have similar lengths in European and non-European countries, housing busts are typically shorter in European countries. The use of a more flexible specification for the hazard function that is based on cubic splines suggests that it evolves in a nonlinear way. From a policy perspective, our study can be useful for predicting the timing and the length of housing boom\textendash bust cycles. Moreover, it highlights the importance of monetary policy by influencing lending rates and affecting the likelihood of occurrence of housing booms.},
  file = {Agnello et al. - 2018 - ECONOMIC ACTIVITY, CREDIT MARKET CONDITIONS, AND T.pdf:/home/gpetrini/Zotero/storage/IKXIIIR3/Agnello et al. - 2018 - ECONOMIC ACTIVITY, CREDIT MARKET CONDITIONS, AND T.pdf:application/pdf},
  journal = {Macroeconomic Dynamics},
  language = {en},
  number = {07},
  timestamp = {2019-03-04T21:21:22Z}
}

