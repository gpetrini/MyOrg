@article{bheringThirlwallLawExternal2019,
  title = {Thirlwall's Law, External Debt Sustainability, and the Balance-of-Payments-Constrained Level and Growth Rates of Output*},
  author = {Bhering, Gustavo and Serrano, Franklin and Freitas, Fabio},
  year = {2019},
