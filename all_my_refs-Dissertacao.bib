  shorttitle = {The {{Bhaduri}}\textendash{{Marglin}} Post-{{Kaleckian}} Model in the History of Distribution and Growth Theories},
  author = {Hein, Eckhard},
  year = {2017},
  month = apr,
  volume = {5},
  pages = {218--238},
  issn = {20495331, 20495323},
  doi = {10.4337/roke.2017.02.05},
  url = {https://www.elgaronline.com/view/journals/roke/5-2/roke.2017.02.05.xml},
  urldate = {2018-09-12},
  abstract = {Starting from a review of the main strands of orthodox and heterodox distribution and growth models and their distinguishing features, with the post-Kaleckian Bhaduri and Marglin (1990) (and Kurz 1990) model as a specific but highly flexible variant of heterodox distribution and growth theories, we develop a simple modelling framework in which we can treat these different theories as different variants of model closure. In a simple closed private one-good economy model, each theory is presented drawing on the relationship between the rate of profit and the rate of growth, as well as on the consideration of one major adjusting variable allowing for the convergence of the endogenous variables of the model to their equilibrium values. This allows for a systematic comparison of exogenous and endogenous variables, of the `logic' or the chain of causalities in each of the approaches, and of the generation of the long-run equilibrium positions of the system. It is finally shown that the post-Kaleckian model is able to cover many but not all of the results generated by the old neoclassical growth model, new neoclassical growth theories, classical\textendash Marxian distribution and growth approaches, and post-Keynesian Kaldor\textendash Robinson and Kalecki\textendash Steindl distribution and growth theories.},
  file = {Hein_2017_The Bhaduri–Marglin post-Kaleckian model in the history of distribution and.pdf:/home/gpetrini/Zotero/storage/NL2FS9XK/Hein_2017_The Bhaduri–Marglin post-Kaleckian model in the history of distribution and.pdf:application/pdf;Snapshot:/home/gpetrini/Zotero/storage/NY66BN9A/roke.2017.02.05.html:text/html},
  groups = {Teorico},
  journal = {Review of Keynesian Economics},
  keywords = {REVER},
  language = {en\_US},
  number = {2},
  timestamp = {2018-09-12T18:34:14Z}
}

