@article{heinPostKeynesianKaleckianMacroeconomic,
  title = {A Post-{{Keynesian}}/{{Kaleckian}} Macroeconomic Model for the Classroom: The Principle of Effective Demand, Distribution Conflict, Inflation and Macroeconomic Policies in a Monetary Economy},
  author = {Hein, Eckhard},
  pages = {69},
  abstract = {We will provide a post-Keynesian macroeconomic model for the classroom. The model is built on the post-Keynesian view of a monetary production economy with creditor-debtor relationships and the endogeneity of the volume of credit and the stock of money. The model contains distribution conflict between firms and workers which will determine the wage and profit shares and may generate conflict inflation, and distribution conflict between rentiers (creditors) and firms (debtors) mainly affected by interest rate policies of the central bank and by (unexpected) inflation generated in the distribution conflict between workers and firms. The model generates a short-run `inflation barrier', which becomes endogenous to demand determined employment and macroeconomic policies in the long run. Based on the model we will provide a full post-Keynesian macroeconomic policy mix as an alternative to the New Consensus Macroeconomics. In this alternative policy package monetary policy should target low long-run real interest rates, take over responsibility for financial stability, and act as a lender of last resort and a guarantor of government debt. Nominal stabilisation should be addressed by income and wage policy, in particular through mediation of distributional conflicts via wage bargaining co-ordination. Real stabilisation and aggregate demand management, both in the short and in the long run, should be the task of fiscal policy, making use of government deficits/surpluses following a `functional finance' approach.},
  file = {Hein - A post-KeynesianKaleckian macroeconomic model for.pdf:/home/gpetrini/Zotero/storage/REKGZW5V/Hein - A post-KeynesianKaleckian macroeconomic model for.pdf:application/pdf},
  groups = {FMM_2019},
  language = {en},
  timestamp = {2019-07-23T12:46:43Z}
}

