@article{maatschAreWeAll2014,
  title = {Are We All Austerians Now? {{An}} Analysis of National Parliamentary Parties' Positioning on Anti-Crisis Measures in the Eurozone},
  shorttitle = {Are We All Austerians Now?},
  author = {Maatsch, Aleksandra},
  year = {2014},
  month = jan,
