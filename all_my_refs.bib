@article{baumgartnerOftenTrustedNever2020,
  title = {Often {{Trusted}} but {{Never}} ({{Properly}}) {{Tested}}: {{Evaluating Qualitative Comparative Analysis}}},
  shorttitle = {Often {{Trusted}} but {{Never}} ({{Properly}}) {{Tested}}},
  author = {Baumgartner, Michael and Thiem, Alrik},
  year = {2020},
  month = may,
  volume = {49},
  pages = {279--311},
  issn = {0049-1241, 1552-8294},
  doi = {10.1177/0049124117701487},
  url = {http://journals.sagepub.com/doi/10.1177/0049124117701487},
  urldate = {2020-09-14},
  abstract = {To date, hundreds of researchers have employed the method of Qualitative Comparative Analysis (QCA) for the purpose of causal inference. In a recent series of simulation studies, however, several authors have questioned the correctness of QCA in this connection. Some prominent representatives of the method have replied in turn that simulations with artificial data are unsuited for assessing QCA. We take issue with either position in this impasse. On the one hand, we argue that data-driven evaluations of the correctness of a procedure of causal inference require artificial data. On the other hand, we prove all previous attempts in this direction to have been defective. For the first time in the literature on configurational comparative methods, we lay out a set of formal criteria for an adequate evaluation of QCA before implementing a battery of inverse-search trials to test how this method performs in different recovery contexts according to these criteria. While our results indicate that QCA is correct when generating the parsimonious solution type, they also demonstrate that the method is incorrect when generating the conservative and intermediate solution type. In consequence, researchers using QCA for causal inference, particularly in human-sensitive areas such as public health and medicine, should immediately discontinue employing the method's conservative and intermediate search strategies.},
  file = {Baumgartner_Thiem_2020_Often Trusted but Never (Properly) Tested.pdf:/HDD/PDFs/2020/undefined/Baumgartner_Thiem_2020_Often Trusted but Never (Properly) Tested.pdf:application/pdf},
  groups = {QCA},
  journal = {Sociological Methods \& Research},
  language = {en},
  number = {2},
  timestamp = {2020-09-14T13:35:15Z}
}

