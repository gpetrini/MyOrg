@article{gaugerResidentialFixedInvestment,
  title = {Residential {{Fixed Investment}} and the {{Macroeconomy}}: {{Has Deregulation Altered Key Relationships}}?},
  author = {Gauger, Jean and Snyder, Tricia Coxwell},
  pages = {20},
  abstract = {Financial deregulation in 1980 potentially altered key relationships between residential \textregistered xed investment (RFI) and key macroeconomic variables. This study uses a vector error correction model to examine relationships between RFI, money, interest rates, and output in pre-deregulation and post-deregulation sub-periods. Results indicate short-term interest rate shocks account for much of RFI variability pre-deregulation. After deregulation, long-term FHA interest rate shocks better account for RFI movements. Results also show that, in the postderegulation era, RFI shocks have increased predictive power for overall gross domestic product movements. Thus, the study \textregistered nds altered relationships between RFI and macroeconomic variables.},
  language = {en},
  timestamp = {2019-12-04T19:18:26Z}
}

