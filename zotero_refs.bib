@article{schwartz_2019_Thinking,
  title = {Thinking about {{Thinking}} about {{Comparative Political Economy}}: From {{Macro}} to {{Micro}} and {{Back}}},
  shorttitle = {Thinking about {{Thinking}} about {{Comparative Political Economy}}},
  author = {Schwartz, Herman Mark and Tranøy, Bent Sofus},
