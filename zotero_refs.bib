@article{huang__Housing,
  title = {Is {{Housing}} the {{Business Cycle}}? A {{Multi}}-Resolution {{Analysis}} for {{OECD Countries}}},
  author = {Huang, Yuting and Li, Qiang and Liow, Kim Hiang and Zhou, Xiaoxia},
  pages = {49},
  abstract = {Fluctuations in the housing market have long been recognized by academics and practitioners as leading indicators of general economic activities. Recently, Leamer (2007, 2015) claims that housing activities both predict and cause national business cycles in the US. We investigate his claims for a larger set of countries. We also examine the predictive relationship in multiple resolutions or time scales, namely shortrun and long-run variations of business cycles. Structural vector auto-regression (SVAR) models are estimated to test his more contentious causal claim. Our results show that: (1) housing indicators lead business cycles in most countries; (2) such leading relationship is the most prominent in the long-run. In addition, our SVAR results for the US indicate that housing factors are likely independent drivers of business cycles. Housing starts predict short-run variations in business cycles, while housing prices predict long-run variations better. The cross-country evidences are less certain. Generally, our findings on timescale based relationships between housing and macro economy put restrictions on future models of monetary transmissions.},
  langid = {english},
  groups = {Cap Fatos Estilizados},
  timestamp = {2019-07-05T19:42:36Z}
}
