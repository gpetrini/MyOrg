@article{nikolaidi_2017_Minsky,
  title = {Minsky {{Models}}: A {{Structured Survey}}},
  shorttitle = {Minsky {{Models}}},
  author = {Nikolaidi, Maria and Stockhammer, Engelbert},
  date = {2017},
  journaltitle = {Journal of Economic Surveys},
  shortjournal = {J. Econ. Surv.},
  volume = {31},
  number = {5},
  pages = {1304--1331},
  issn = {1467-6419},
  doi = {10.1111/joes.12222},
  abstract = {Minsky's ideas have recently gained prominence in the mainstream as well as in the heterodox literature. However, there exists no agreement upon the formal presentation of Minsky's insights. The aim of this paper is to survey the literature and identify differences and similarities in the ways through which Minskyan ideas have been formalized. We distinguish between the models that focus on the dynamics of debt or interest, with no or a secondary role for asset prices, and the models in which asset prices play a key role in the dynamic behavior of the economy. Within the first category of models, we make a classification between (i) the Kalecki–Minsky models, (ii) the Kaldor–Minsky models, (iii) the Goodwin–Minsky models, (iv) the credit rationing Minsky models, (v) the endogenous target debt ratio Minsky models and (vi) the Minsky–Veblen models. Within the second category of models, we distinguish between (i) the equity price Minsky models and (ii) the real estate price Minsky models. Key limitations of the models and directions for future research are outlined.},
  langid = {english},
  keywords = {Business cycles,Debt cycles,Financial instability,Post-Keynesian economics},
  annotation = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1111/joes.12222},
  groups = {FIH},
  timestamp = {2020-07-21T13:07:34Z}
}
