@article{hardie_2013_Banks,
  title = {Banks and the {{False Dichotomy}} in the {{Comparative Political Economy}} of {{Finance}}},
  author = {Hardie, Iain and Howarth, David and Maxfield, Sylvia and Verdun, Amy},
  date = {2013-10},
