@article{henkel_2015_Winner,
  title = {And the Winner Is—{{Acquired}}. {{Entrepreneurship}} as a Contest Yielding Radical Innovations},
  author = {Henkel, Joachim and Rønde, Thomas and Wagner, Marcus},
  date = {2015-03-01},
  journaltitle = {Research Policy},
