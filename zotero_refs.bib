@article{dweck_2019_This,
  title = {☆{{This}} Research Did Not Receive Any Specific Grant from Funding Agencies in the Public, Commercial, or Not-for-Profit Sectors.{{Discussing}} the Role of Fiscal Policy in a Demand-Led Agent-Based Growth Model},
  author = {Dweck, Esther and Vianna, Matheus Trotta and da Cruz Barbosa, Arthur},
