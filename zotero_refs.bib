@report{botta_2019_When,
  title = {When Complexity Meets Finance: A Contribution to the Study of the Macroeconomic Effects of Complex Financial Systems},
  shorttitle = {When Complexity Meets Finance},
  author = {Botta, Alberto and Caverzasi, Eugenio and Russo, Alberto},
  date = {2019-04},
