@article{liResearchImprovementTechnical2020,
  title = {Research on the Improvement of Technical Efficiency of {{China}}'s Property Insurance Industry: A Fuzzy-Set Qualitative Comparative Analysis},
  shorttitle = {Research on the Improvement of Technical Efficiency of {{China}}'s Property Insurance Industry},
  author = {Li, Zhiguang and Li, Yaokuang and Long, Dan},
  year = {2020},
